{
  "template": {
    "mappings": {
      "properties": {
        "@timestamp": {
          "format": "date_optional_time||epoch_second||strict_date_time",
          "index": true,
          "ignore_malformed": false,
          "store": false,
          "type": "date",
          "doc_values": true
        },
        "message": {
        "type": "wildcard"
        }
      }
    }
  },
  "_meta": {
    "description": "Mappings for @timestamp and message fields"
  }
}
