  "name": "apache-1",
  "description": "",
  "namespace": "default",
  "inputs": {
    "apache-logfile": {
      "enabled": true,
      "streams": {
        "apache.access": {
          "enabled": true,
          "vars": {
            "paths": [
              "/var/log/apache2/access.log*",
              "/var/log/apache2/other_vhosts_access.log*",
              "/var/log/httpd/access_log*"
            ],
            "tags": [
              "apache-access"
            ],
            "preserve_original_event": false
          }
        },
        "apache.error": {
          "enabled": true,
          "vars": {
            "paths": [
              "/var/log/apache2/error.log*",
              "/var/log/httpd/error_log*"
            ],
            "tags": [
              "apache-error"
            ],
            "preserve_original_event": false
          }
        }
      }
    },
